        <TextBox x:Name="FIO" HorizontalAlignment="Left" Height="37" Margin="120,23,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="168" FontSize="20"/>
        <Label Content="ФИО" HorizontalAlignment="Left" Margin="9,24,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="33" Width="100" FontSize="20"/>
        <Label Content="Номер" HorizontalAlignment="Left" Margin="9,74,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="33" Width="100" FontSize="20"/>
