        <TextBox x:Name="Numb" HorizontalAlignment="Left" Height="37" Margin="120,76,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="168" FontSize="20"/>
        <TextBox x:Name="Login" HorizontalAlignment="Left" Height="37" Margin="120,127,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="168" FontSize="20"/>
        <TextBox x:Name="Pass" HorizontalAlignment="Left" Height="37" Margin="120,181,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="168" FontSize="20"/>

    </Grid>
</Window>
